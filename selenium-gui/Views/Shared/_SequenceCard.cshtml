<div class="card text-center">
    <div class="card-header">
        <div class="row">
            <div class="col">
                <button type="button" id="import-sequence" class="btn btn-outline-primary" data-toggle="modal" data-target="#import-modal">Import</button>
            </div>
            <div class="col">
                <h2>Sequence</h2>
            </div>
            <div class="col">
                <button type="button" id="export-sequence" class="btn btn-outline-primary" data-toggle="modal" data-target="#export-modal">Export</button>
            </div>
        </div>
    </div>
    <form>
        <ul id="sequence-list" class="list-group list-group-flush">
            <partial name="_step" />
        </ul>

        <div class="card-footer">
            <div class="row">
                <div class="col">
                    <button type="button" id="run-sequence" class="btn btn-success">Run Sequence</button>
                </div>
                <div class="col">
                    <button type="button" id="add-step" class="btn btn-primary">Add Step</button>
                </div>
                <div class="col">
                    <button class="btn btn-info">Get Script</button>
                </div>
            </div>
        </div>
    </form>
</div>


<partial name="_Import_Sequence_Modal" />

<partial name="_Export_Sequence_Modal" />